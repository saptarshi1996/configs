{
  "workbench.startupEditor": "none",
  "editor.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "editor.fontFamily": "Hack Nerd Font Mono",
  "terminal.integrated.fontFamily": "Hack Nerd Font Mono",
  "editor.lineHeight": 1.35,
  "terminal.integrated.lineHeight": 1.35,
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "git.autofetch": true,
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "git.confirmSync": false,
  "window.titleBarStyle": "custom",
  "explorer.confirmDelete": false,
  "search.useIgnoreFiles": false,
  "extensions.ignoreRecommendations": true,
  "editor.wordWrap": "on",
  "editor.overviewRulerBorder": false,
  "python.createEnvironment.trigger": "off",
  "workbench.editor.showIcons": true,
  "explorer.confirmDragAndDrop": false,
  "editor.semanticHighlighting.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": false
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.type.class",
          "entity.name.class",
          "entity.name.type.module",
          "support.class",
          "support.type"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.cursorSurroundingLines": 8,
  "editor.renderLineHighlight": "gutter",
  "editor.stickyScroll.enabled": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/venv": true,
    "**/.venv": true,
    "**/.next": true,
    "**/.aws-sam": true
  },
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "gitlens.plusFeatures.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.minimap.renderCharacters": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "black-formatter.args": [
    "--line-length",
    "70"
  ],
  "window.zoomLevel": 0,
  "workbench.secondarySideBar.showLabels": false,
  "terminal.integrated.fontWeightBold": "normal",
  "chat.commandCenter.enabled": false,
  "workbench.navigationControl.enabled": false,
  "workbench.iconTheme": "vscode-great-icons",
  "cSpell.enabledNotifications": {
    "Average Word Length too Long": false,
    "Lines too Long": false,
    "Maximum Word Length Exceeded": false
  },
  "cSpell.ignoreWords": [],
  "workbench.experimental.share.enabled": true,
  "window.commandCenter": false,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "workbench.activityBar.location": "top",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "lineBreakInsert"
      ],
      "silent": true
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  }
}